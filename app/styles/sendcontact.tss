	width: '100%',
	height: 280,
	font: {fontSize: 15}
},
"#sendButton": {
	title: 'Send',
	top: 100,
   	borderColor: '#000',
   	borderRadius:10,
   	backgroundGradient: {
    	type:'linear',
   		colors:['#195280','#003F72'],
