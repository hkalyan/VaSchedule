"#sendcontact": {
	
	top: 0,
	zIndex: 100,
	title: 'Send Contact Information',
	layout: 'vertical',
	backgroundColor: '#fff',
	navBarHidden: false
},

"#subjectLabel": {
	top: 35,
	color: "#444",
	textAlign: Titanium.UI.TEXT_ALIGNMENT_LEFT,
	left: 0,
	font : { fontSize: 24, font: 'sans-serif',fontWeight: 'bold' },
	text: "Subject:"
},
"#messageLabel": {
	top: 35,
	color: "#444",
	textAlign: Titanium.UI.TEXT_ALIGNMENT_LEFT,
	left: 0,
	font : { fontSize: 24, font: 'sans-serif',fontWeight: 'bold' },
